<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.worldstory.view.ducactivity.DucChapterMarkedActivity"
    android:orientation="vertical"
    android:fitsSystemWindows="true"
    >
    <ImageButton
        android:id="@+id/btn_back_chapterMarked"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:src="@drawable/icon_back"
        android:background="@drawable/image_button_search_slector"
        />
    <TextView
        android:layout_gravity="center"
        android:text="@string/dataNotFound"
        android:id="@+id/txt_noti_chapterMarked"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        />
    <androidx.swiperefreshlayout.widget.SwipeRefreshLayout
        android:id="@+id/swipeRefresh_chapterMarked"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >
        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/rv_chapter_chapterMarked"
            android:layout_width="match_parent"
            android:layout_height="match_parent"

            >

        </androidx.recyclerview.widget.RecyclerView>
    </androidx.swiperefreshlayout.widget.SwipeRefreshLayout>

</LinearLayout>